<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="630" viewBox="0 0 1200 630">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e40af;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="0" dy="4" stdDeviation="8" flood-color="#000000" flood-opacity="0.25"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bg)"/>
  
  <!-- Content container -->
  <g filter="url(#shadow)">
    <!-- Main icon -->
    <rect x="100" y="150" width="120" height="120" rx="24" fill="white" opacity="0.1"/>
    <rect x="110" y="160" width="100" height="100" rx="20" fill="white"/>
    
    <!-- File tree in icon -->
    <g fill="#2563eb">
      <rect x="125" y="180" width="8" height="6" rx="1"/>
      <rect x="133" y="180" width="20" height="6" rx="1"/>
      
      <rect x="129" y="186" width="2" height="40"/>
      
      <rect x="131" y="195" width="2" height="6"/>
      <rect x="133" y="197" width="6" height="2" rx="0.5"/>
      <rect x="139" y="195" width="15" height="6" rx="1"/>
      
      <rect x="135" y="210" width="2" height="6"/>
      <rect x="137" y="212" width="6" height="2" rx="0.5"/>
      <rect x="143" y="210" width="18" height="6" rx="1"/>
      
      <rect x="135" y="225" width="2" height="6"/>
      <rect x="137" y="227" width="6" height="2" rx="0.5"/>
      <rect x="143" y="225" width="15" height="6" rx="1"/>
    </g>
  </g>
  
  <!-- Title -->
  <text x="280" y="220" font-family="Arial, sans-serif" font-size="48" font-weight="bold" fill="white">
    File Structure Editor
  </text>
  
  <!-- Subtitle -->
  <text x="280" y="270" font-family="Arial, sans-serif" font-size="24" fill="white" opacity="0.9">
    Parse, Edit &amp; Download Tree Hierarchies
  </text>
  
  <!-- Description -->
  <text x="280" y="320" font-family="Arial, sans-serif" font-size="18" fill="white" opacity="0.8">
    Ultimate file structure editor for parsing ASCII trees,
  </text>
  <text x="280" y="345" font-family="Arial, sans-serif" font-size="18" fill="white" opacity="0.8">
    visual editing, and ZIP generation.
  </text>
  
  <!-- Features -->
  <g fill="white" opacity="0.7" font-family="Arial, sans-serif" font-size="16">
    <circle cx="290" cy="420" r="3" fill="white"/>
    <text x="305" y="425">Parse ASCII file trees</text>
    
    <circle cx="290" cy="450" r="3" fill="white"/>
    <text x="305" y="455">Visual tree editing</text>
    
    <circle cx="290" cy="480" r="3" fill="white"/>
    <text x="305" y="485">Export as ZIP files</text>
    
    <circle cx="290" cy="510" r="3" fill="white"/>
    <text x="305" y="515">Developer-friendly tools</text>
  </g>
  
  <!-- Code preview -->
  <g transform="translate(650, 150)">
    <rect width="450" height="300" rx="12" fill="rgba(0,0,0,0.3)" stroke="rgba(255,255,255,0.2)"/>
    <rect x="20" y="20" width="410" height="260" rx="8" fill="rgba(0,0,0,0.8)"/>
    
    <!-- Code content -->
    <text x="35" y="50" font-family="Monaco, monospace" font-size="14" fill="#10b981">project/</text>
    <text x="35" y="70" font-family="Monaco, monospace" font-size="14" fill="#6b7280">├── src/</text>
    <text x="35" y="90" font-family="Monaco, monospace" font-size="14" fill="#6b7280">│   ├── components/</text>
    <text x="35" y="110" font-family="Monaco, monospace" font-size="14" fill="#f59e0b">│   │   ├── Header.tsx</text>
    <text x="35" y="130" font-family="Monaco, monospace" font-size="14" fill="#f59e0b">│   │   └── Footer.tsx</text>
    <text x="35" y="150" font-family="Monaco, monospace" font-size="14" fill="#f59e0b">│   ├── App.tsx</text>
    <text x="35" y="170" font-family="Monaco, monospace" font-size="14" fill="#f59e0b">│   └── main.tsx</text>
    <text x="35" y="190" font-family="Monaco, monospace" font-size="14" fill="#6b7280">├── public/</text>
    <text x="35" y="210" font-family="Monaco, monospace" font-size="14" fill="#8b5cf6">│   └── index.html</text>
    <text x="35" y="230" font-family="Monaco, monospace" font-size="14" fill="#ef4444">├── package.json</text>
    <text x="35" y="250" font-family="Monaco, monospace" font-size="14" fill="#ef4444">└── README.md</text>
  </g>
</svg>